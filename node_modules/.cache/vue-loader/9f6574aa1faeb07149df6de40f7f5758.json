submit\" \n      value=\"Add\" \n      @click=\"addMessage({ subject: subject, body: msgBody })\" \n      :disabled=\"!subject || !msgBody\">\n\n    <hr/>\n    <h3>Messages on Database</h3>\n    <p v-if=\"messages.length === 0\">No Messages</p>\n    <div class=\"msg\" v-for=\"(msg, index) in messages\" :key=\"index\">\n        <p class=\"msg-index\">[{{index}}]</p>\n        <p class=\"msg-subject\" v-html=\"msg.subject\"></p>\n        <p class=\"msg-body\" v-html=\"msg.body\"></p>\n        <input type=\"submit\" @click=\"deleteMessage(msg.pk)\" value=\"Delete\" />\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapState, mapActions } from 'vuex'\n\nexport default {\n  name: \"Messages\",\n  data() {\n    return {\n      subject: \"\",\n      msgBody: \"\",\n    };\n  },\n  computed: mapState({\n    messages: state => state.messages.messages\n  }),\n  methods: mapActions('messages', [\n    'addMessage',\n    'deleteMessage'\n  ]),\n  created() {\n    this.$store.dispatch('messages/getMessages')\n  }\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\nhr {\n  max-width: 65%;\n}\n\n.msg {\n  margin: 0 auto;\n  max-width: 30%;\n  text-align: left;\n  border-bottom: 1px solid #ccc;\n  padding: 1rem;\n}\n\n.msg-index {\n  color: #ccc;\n  font-size: 0.8rem;\n  /* margin-bottom: 0; */\n}\n\nimg {\n  width: 250px;\n  padding-top: 50px;\n  padding-bottom: 50px;\n}\n\n</style>\n"]}]}